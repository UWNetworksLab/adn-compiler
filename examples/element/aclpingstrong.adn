internal
{
    @consistency(strong) @combiner(sum) @persistence(true)
	acl: Map<string, string>
}

fn init() {
	acl.set('Apple', 'Yes');
	acl.set('Banana', 'No');
}

fn req(rpc_req) {
    res := acl.get(rpc_req.get('body'));
	match (res) {
		Some(permission) => {
			match (permission) {
				'Yes' => {
					send(rpc_req, NET);
				}
				'No' => {
					send(err('acl'), APP);
				}
				_ => {
				}
			};
		}
		None => {
			send(err('acl'), APP);
		}
	}; 
}

fn resp(rpc_resp) {
    send(rpc_resp, APP);	
}