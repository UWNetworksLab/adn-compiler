internal
{
	acl: Map<string, string>
}

fn init() {
	acl.set('Apple', 'Yes');
	acl.set('Banana', 'No');
}

fn req(rpc_req) {
	match (acl.get(rpc_req.get('name'))) {
		Some(permission) => {
			match (permission) {
				'Yes' => {
					send(rpc_req, NET);
				}
				'No' => {
					send(err('acl'), APP);
				}
			};
		}
		None => {
			send(err('acl'), APP);
		}
	}; 
}

fn resp(rpc_resp) {
    send(rpc_resp, APP);	
}