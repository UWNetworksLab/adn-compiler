internal
{
	record: Map<string, Instant>
  latency: Vec<float>
}

fn init() {
}

fn req(rpc_req) {
	record.set(rpc_req.get('body'), current_time());
	send(rpc_req, NET);
}

fn resp(rpc_resp) {
  rpc_body := rpc_resp.get('body');
  match(record.get(rpc_body)) {
    Some(t) => {
      lat := time_diff(current_time(), t);
      latency.set(latency.size(), lat);
    }
    None => {
    }
  };
  send(rpc_resp, APP);
}
